                        regexLiterals: !0
                    }),
                A = {};
            k(O, ["default-code"]);
            k(x([], [
                        ["pln", /^[^<?]+/],
                        ["dec", /^<!\w[^>]*(?:>|$)/],
                        ["com", /^<\!--[\S\s]*?(?:--\>|$)/],
                        ["lang-", /^<\?([\S\s]+?)(?:\?>|$)/],
                        ["lang-", /^<%([\S\s]+?)(?:%>|$)/],
                        ["pun", /^(?:<[%?]|[%?]>)/],
                        ["lang-", /^<xmp\b[^>]*>([\S\s]+?)<\/xmp\b[^>]*>/i],
                        ["lang-js", /^<script\b[^>]*>([\S\s]*?)(<\/script\b[^>]*>)/i],
                        ["lang-css", /^<style\b[^>]*>([\S\s]*?)(<\/style\b[^>]*>)/i],
                        ["lang-in.tag", /^(<\/?[a-z][^<>]*>)/i]
                    ]), "default-markup,htm,html,mxml,xhtml,xml,xsl".split(","));
            k(x([
                        ["pln", /^\s+/, q, " \t\r\n"],
                        ["atv", /^(?:"[^"]*"?|'[^']*'?)/, q, "\"'"]
                    ], [
                        ["tag", /^^<\/?[a-z](?:[\w-.:]*\w)?|\/?>$/i],
                        ["atn", /^(?!style[\s=]|on)[a-z](?:[\w:-]*\w)?/i],
                        ["lang-uq.val", /^=\s*([^\s"'>]*(?:[^\s"'/>]|\/(?=\s)))/],
                        ["pun", /^[/<->]+/],
